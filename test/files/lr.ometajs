ometa LeftRecursion {
  number = <digit+ (('e' | 'E') ('-' | '+')? digit+)?>:f -> [#number, parseFloat(f)]
         | <digit+ ('.' digit+)?>:f                      -> [#number, parseFloat(f)],
  expr = expr:a "+" number:b -> ['+', a, b]
       | expr:a "-" number:b -> ['-', a, b]
       | number:a -> a,
  exprs = exprs:a "," expr:b -> [',', a, b]
        | expr:b -> b
}
